@font-face {
		font-family: 'WalkwayRounded';
		src: url('../fonts/walkway_rounded.ttf');
}
@font-face {
		font-family: 'JosefinSans_Bold';
		src: url('../fonts/JosefinSans_Bold.ttf');
}
@font-face {
		font-family: 'Oswald_Regular';
		src: url('../fonts/Oswald_Regular.ttf');
}
@font-face {
		font-family: 'LeagueGothic_Regular';
		src: url('../fonts/LeagueGothic_Regular.ttf');
}


body {
	padding: 185px 0 0 0;
	margin: 0px;
	font-family: 'Oswald_Regular';
	overflow: auto;
}

a {
	color: #F84A0E;
}

.home_banner a, .header a {
	color: white;
	text-decoration: none;
	font-weight: bold;
	font-size: 16px;
}

.switch_link a {
	text-decoration: none;
	font-size: 14px;
	display: inline;
}

#flash_messages {
	margin:15px;
}


#footer {
   position:absolute;
   bottom:0;
   width:100%;
   text-align: center;
   height:60px;   /* Height of the footer */	
   margin: -10px !important;  
   background:black;
}

#footer ul {
	list-style:none;
	margin-bottom:10px;
}
		
#footer ul li {
	display:inline;
}
			
#footer ul li a {
	font-size:10px;
	text-transform:uppercase;
	text-decoration:none;
	border-right: 1px solid white;
	padding: 0 10px;
	color: white;	
}
				
#footer ul li a.right {
	border-right:none;
}
		
#footer p {
	font-size:10px;
}
		
#footer p a{
	font-size:10px;
	text-decoration:none;
}
			
#footer p a:hover {
	text-decoration:underline;
}

#content {
	min-height: 100%;
	width: 100%;
	box-sizing: border-box;
	padding: 10px 10px 85px 10px;
	position: absolute;
}

#manage_account {
	font-family: 'LeagueGothic_Regular';
	font-size: 24px;
}

.home_logo {
	position: relative;
	top: 43px;
	left: 50px;
	height: 100px !important;
	width: 100px !important;
}
.company_text {
	top: -15px;
	position: relative;
}
.company_name {
	margin-left: 40px;
	font-size: 28px;
	
}
.company_blurb {
	margin-left: 147px;
	font-size: 18px;
}

.white {
	width: 100%;
	background: white;
	height: 100px;
	top: -151px;
	position: relative;
	z-index: -1;
}

.home_banner {
	padding-top:5px;
}

.overlay {
	padding: 0px !important;
	border-top: 10px black solid !important;
	z-index: 1;
}

.overlay_logo_signup {
	width: 75px;
	height: 75px;
	display: block;
	position:absolute;
	top: -8px;
	left: 4px;
}

.overlay_logo_login {
	width: 75px;
	height: 75px;
	display: block;
	position:absolute;
	top: -30px;
	left: -75px;
}

#sign_up {
	overflow:hidden;
	display:none;
}
.sign_up_form {
	position: relative;
	left: 60px;
}

#log_in {
	overflow:hidden;
	display:none;
}

.log_in_form {
	position: relative;
	left: 60px;
}

.log_in_form a {
	text-decoration: underline;
	color: red !important;
}

.login_btn {
	left: -118px;
	top: -25px;
}

.login_bottom_fields{
	line-height: 25px;
}

.overlay input[type="submit"] {
	float:right;
	position: relative;
}

.submit_btn {
	background:#F84A0E;
	color: white;
	cursor: pointer;		
	height: 50px;	
}


.featured_header {
	font-family: 'Oswald_Regular';
}

.place_bet {
	background:#F84A0E;
	color: white;
	cursor: pointer;
	height: 50px;
	position: relative;
}

.create_accnt {
	left: -129px;
}

input[type="submit"]:hover {
	
}

.close_button {
	float: right;
	cursor: pointer;
	padding: 0;
	margin: 0;
	left: -60px;
	top: -20px;
	display: inline-block;
	position: relative;
}

.header{
	position: fixed;
	top: 0;
	width: 100%;
	height: 55px;
	z-index: 11;
	background: black; 
	background-repeat: repeat-x;
}

.user_nav {
	float: right;
	margin-top: 10px;
	margin-bottom:10px;
	margin-left: 10px;
	margin-right: 50px;
}

.button {
	-moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    -khtml-border-radius: 5px;
    border-radius: 5px;	
}

.user_nav .button {
	-moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    -khtml-border-radius: 10px;
    border-radius: 10px;	
    height: 40px;     
}

.user_nav > .button:hover {
	background-color: yellow;
}

.user_nav > .logout_btn {
	background-color: #F84A0E;
	color: white;
	text-shadow: 1px 1px 1px black;
	font-weight: bold;	
}

.user_nav > .logout_btn:hover {
	background-color: black;
}

.overlay h1 {
	font-size: 20px;
	font-weight: bold;
	line-height: 0px;
}

.overlay h6 {
	line-height:0px;
}

.overlay label {
	display: inline-block;	
	text-align: right;
}

.reg_label {
	display: inline-block;	
	text-align: right;
	width: 92px;
}

.overlay .reg_label {
	width: 96px;
}

.overlay .login_label {
	width: 83px;
}

.overlay input[type="text"], input[type="password"] {
	display: inline;
	border: 2px solid black;
}

.terms_link {
	text-decoration: underline;
	color: red !important;
}

.dropdown_arrow {
	position: relative;
	top: 3px;
	height: 16px;
	width: 16px;
}

#top_ten_users_table td {
	width: 100px;
	text-align: center;
	border-bottom: 1px dotted black;
	height: 25px;
	font-family: 'JosefinSans_Bold';
}

#users_table {
	font-family: 'JosefinSans_Bold';
}

#users_table td {
	width: 165px;
	text-align: center;
	height: 30px;	
}

#users_table th {
	font-weight: normal !important;
	text-decoration: none;
	font-size: 24px;
	line-height: 55px;
	text-decoration: underline;
}

#users_table th a.current {
	font-weight: bold !important;
	padding-right: 25px;
    background-repeat: no-repeat;
	background-position: right 95%;
}
#users_table th .asc { 
  background-image: url(<%= asset_path 'up_arrow.png' %>); 
}

#users_table th .desc { 
  background-image: url(<%= asset_path 'down_arrow.png' %>); 
}

.duck_pagination { padding: 5px; margin: 15px; letter-spacing: 2px; float: center;} 
.duck_pagination a { padding: 2px 5px 2px 5px; margin: 2px; border: 1px solid #aaaadd; text-decoration: none; color: #000099; } 
.duck_pagination a:hover, 
.duck_pagination a:active { border: 1px solid #000099; color: #000; } 
.duck_pagination span.current { padding: 2px 5px 2px 5px; margin: 2px; border: 1px solid #000099; font-weight: bold; background-color: #000099; color: #FFF; } 
.duck_pagination span.disabled { padding: 2px 5px 2px 5px; margin: 2px; border: 1px solid #eee; color: #ddd; }
 

#featured_games_table td {	
	padding: 0;
	width: 100px;
	text-align: center;
	border-bottom: 1px dotted black;
	height: 65px;
	font-family: 'JosefinSans_Bold';
}

#featured_games_table tr {
	font-family: 'JosefinSans_Bold';
}

.featured_date {
	font-family: 'JosefinSans_Bold';
	color: white;
	display: block;
	text-align: center;
	width: 550px;
	background: black;
	font-weight: bold;
	font-size: 18px;
}

.featured_team_name {
	font-size:28px; 
	font-family: 'Oswald_Regular';
}

.sports_list {
    margin: 0 auto;
    padding: 0;    
    width: 105px;
	position: absolute;
	top: 130px;
	left: 80px;
	z-index: 9999999999;
	background: white;
	border: 1px solid black;
	display: none;
}

.home_page_list li::before {

}
.sports_list a{
	text-decoration: none;
}
.sports_list li{
	display: block;
    text-align: left;
    list-style: none;
    padding: 3px 10px 3px 10px;
    margin: 5px;
    border: 1px solid #CCC;
}

.sports_list li:hover {
	background-color: #CD3030;
}

.home_page_list {
	font-family: 'LeagueGothic_Bold';
	list-style: none;
	clear:both;
	width: 100%;
	height: 35px;
	background: black;
}

.home_page_list li {
	color: white;
	float: left;
	width: auto;
	margin: 10px;
}

.number_input {
	width: 148px;
	vertical-align: middle;
	margin: 10px;
	font-size: 36px;
}

.wager_link_home {
	color: white;
	background: black;
	height: 45px;
	display: block;
	font-size: 24px;
	text-decoration: none;	
}

#profile span {
	display:inline-block;
}

.black_gradient {
	background-image: -ms-linear-gradient(bottom right, #000000 0%, #3D3D3D 100%);
	background-image: -moz-linear-gradient(bottom right, #000000 0%, #3D3D3D 100%);
	background-image: -o-linear-gradient(bottom right, #000000 0%, #3D3D3D 100%);
	background-image: -webkit-gradient(linear, right bottom, left top, color-stop(0, #000000), color-stop(1, #3D3D3D));
	background-image: -webkit-linear-gradient(bottom right, #000000 0%, #3D3D3D 100%);
	background-image: linear-gradient(to top left, #000000 0%, #3D3D3D 100%);
}

.box_number {
	border: 1px dashed black;
	width: 100px;
	height: 100px;
	padding: 5px;
	line-height: 0px;
}

.box_number span {
	font-weight: bold;
	font-size: 14px;
	height:0px;
	margin: 0px;
}

.box_number div {
	text-align:center;
	margin-top: 45px;
	font-size: 30px;
	vertical-align: middle;
}

#flash_notice {
	color: rgb(125,203,31);
}

#flash_alert {
	color: #F84A0E;
}

.missing_pic {
	height: 200px;
	width: 200px;
	margin-right: 30px;
	float: left;
}

.profile_pic {
	margin-right: 30px;
	float: left;
}

.profile_header {
	height: 225px;
	border-bottom: 5px solid black;
}

.upload_text {
	padding-top: 25px;
	display: inline-block;
}

.upload_btn {
	width: 190px;
	height: 50px;
	cursor: pointer;
}

.change_upload_btn {
	width: 50px;
	display: inline;
	vertical-align: middle;
	cursor: pointer;
}

.hidden_upload_field {
	display:none;
}

.basic_header{
	width:100%;
	border-bottom: 3px solid black;
}

#parlay_game_list {
	margin-left:45px;
}

#parlay_game_table {
	text-align:left;
}

#parlay_game_table td {
	width:500px;
}

.spread_text {
	font-size: 14px;
}

.add_border {
	border-bottom: 1px black solid;
}

#parlay_left {
	width: 75%;
}

#parlay_wager {
	background: #A8A8A8;
	line-height: 32px;
	float: right;
	margin-right: 5px;
	width: 25%;
	position: fixed;
	right: 0;
	font-size: 20px;
	padding: 10px;
}

.table_left {
	width: 100px !important;
}

.table_right {
	text-align: center;
	width: 93px !important;
}

.winning_payout {
	color: green;
	font-size: 36px;
	vertical-align: middle;
	
}

#track_record_graph {width: 500px; height: 200px; margin-left: 30px; }
#track_record_graph td, #track_record_graph th {  font-size: 1.2em; padding: 2px; width: 13%; }
#track_record_graph th { background-color:#f4f4f4; } 
#track_record_graph caption { font-size: 1.5em;  }

.visualize-labels-x, .visualize-labels-y {
	font-size:10px;
}


.public_bet {
	color: green;
	font-weight: bold;
}

.private_bet {
	color: red;
	font-weight: bold;
}
