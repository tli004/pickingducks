 	<div id="bet_history">
 		<% if @past_twenty.length > 0 %>
	 		<table class="past_wagers_table">
	 			<thead>
			 		<tr>
			 			<th>Gameday</th>
			 			<th>Event</th>
			 			<th>Final Score</th>
			 			<th>Pick</th>
			 			<th>Amount</th>		 			
			 			<th>Outcome</th>
			 			<th>Ducks+-</th>
			 			<th>Resulting Bankroll</th>
			 		</tr>
			 	</thead>	
	 		<% @past_twenty.each do |bet| %>
	 		<tbody>
	 			<tr class="past_bet_info">
 				<td>
 					<%= bet.event.start_time.strftime("%m-%d") %>
 				</td>
 				<% if !bet.event.spread.nil? %>
	 				<td><%= "#{bet.event.away_team}(#{-bet.event.spread})@#{bet.event.home_team}(#{bet.event.spread})"%></td>
	 			<% else %>
	 				<td><%= "#{bet.event.away_team}(#{bet.event.moneyline_away})@#{bet.event.home_team}(#{bet.event.moneyline_home})"%></td>
	 			<% end %>	 
	 			<td><%= "#{bet.event.away_total.round(0)}-#{bet.event.home_total.round(0)}" %></td>			
	 			<td><%= bet.event.bet_type(bet.bet_type) %></td>
	 			<td><%= bet.amount %>d</td> 				
	 			<% if bet.winning? %>
	 				<td style="color:green">WIN</td>
	 			<% else %>
	 				<td style="color:red">LOSS</td>
	 			<% end %>
	 			<% if !bet.winning? %>
	 				<td>(-)<%= bet.amount %></td>
	 			<% else %>
		 			<% if !bet.event.spread.nil? %>	 				
		 				<td>(+)<%= bet.amount %></td>
		 			<% else %>
		 			  <% if bet.bet_type == "home" %>
			                <% if bet.event.moneyline_home > 0 %>
			                  <td>(+)<%= bet.amount * bet.event.moneyline_home / 100 %></td>
			                <% else %>
			                  <td>(+)<%= bet.amount * 100 / bet.event.moneyline_home.abs %></td>
			                <% end %>
		              <% elsif bet.bet_type == "away" %>
		                <% if bet.event.moneyline_away > 0 %>
		                  <td>(+)<%= bet.amount * bet.event.moneyline_away / 100 %></td>
		                <% else %>
		                  <td>(+)<%= bet.amount * 100 / bet.event.moneyline_away.abs %></td>
		                <% end %>
		              <% else %>
		 				<td>(+)<%= bet.amount %></td>
		 			  <% end %>
		 			<% end %>
		 		<% end %>
		 		<td><%= bet.resulting_bankroll %>d</td>
	 			</tr>
	 		</tbody>
	 		<% end %>
	 		</table>
	 		<br />
	 		<br />
	 		<h1>Past 30 Days</h1>
	 		<table class="user_calendar" cellspacing="0">
				<thead>
					<tr>
						<%= set_calendar_days(30.days.ago.strftime("%w").to_i) %>
					</tr>
				</thead>
				<tbody>
					<tr>
						<td class="padding" colspan="3"></td>
						<% 30.downto(1) do |i| %>
							<% date_bets = current_user.bets.where("closed_at between ? and ?", i.days.ago.beginning_of_day, i.days.ago.end_of_day) %>
							<td class="date_has_event">
								<% if date_bets.length > 0 %>							
										<div class="events">
											<ul>
												<% total = 0 %>
												<% date_bets.each do |bet| %>
												<li>
													<span class="title">
														<% if !bet.event.spread.nil? %>
												 			<%= "#{bet.event.away_team}(#{-bet.event.spread}) @ #{bet.event.home_team}(#{bet.event.spread})" %></span>
										 				<% else %>
												 			<%= "#{bet.event.away_team}(#{bet.event.moneyline_away}) @ #{bet.event.home_team}(#{bet.event.moneyline_home})" %></span>
										 				<% end %>
													</span>
													<span class="desc">												
														<%= "Total Points: #{bet.event.total_points}" %><br />
														<%= "You put #{bet.amount} ducks on #{bet.event.bet_type(bet.bet_type)}" %><br />
														<% if bet.winning? %>
															<% total += bet.amount %>
															You Won!
														<% else %>
															<% total -= bet.amount %>
															You Lost...
														<% end %>
													</span>
												</li>
												<% end %>
											</ul>
										</div>	
									<%= total >= 0 ? "+#{total}" : total %>
									<% if total > 0 %>
										<script type="text/javascript">
											var scripts = document.getElementsByTagName( 'script' );
											var thisScriptTag = scripts[ scripts.length - 1 ];
											$(thisScriptTag).parent().addClass("green");
										</script>
									<% else %>
										<script type="text/javascript">
											var scripts = document.getElementsByTagName( 'script' );
											var thisScriptTag = scripts[ scripts.length - 1 ];
											$(thisScriptTag).parent().addClass("red");
										</script>
									<% end %>
								<% else %>
									+0						
								<% end %>
							</td>
							<% if i.days.ago.strftime("%w").to_i == ((30.days.ago.strftime("%w").to_i + 3) % 7) %>		
							</tr>
							<tr>
							<% end %>
						<% end %>
						<td class="padding"></td>
					</tr>
				</tbody>
				<tfoot>
					<%= set_calendar_days(30.days.ago.strftime("%w").to_i) %>
				</tfoot>
			</table>
	 	<% else %>
	 	No pick history.
	 	<% end %>
